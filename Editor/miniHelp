
основные переменные для каждого объекта:

переменные для процессов

//время выполнения стадии процессора, базовая часть (после завершения вызывается следующая стадия)
SET_INT_V(0,@"TimeBaseTimer"),
//время выполнения стадии процессора, случайная часть
SET_INT_V(1,@"TimeRndTimer"),

//время ожидания стадии процессора, базовая часть
SET_INT_V(0,@"TimeBaseDelay"),
//время ожидания стадии процессора перед выполнением(пауза), случайная часть.
SET_INT_V(1,@"TimeRndDelay"),

//установка селекторя для инициализации стадии
SET_STRING_V(@"",@"SelectorInit"),
//установка селекторя для подготовки стадии
SET_STRING_V(@"",@"SelectorPrepare"),

